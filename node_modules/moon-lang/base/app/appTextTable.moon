crossWith = zb2rhgvKtMNqZhEKgoyrxo71upjNNdBAJdeQdQe3Uwy2ySyzV
map = zb2rhgW1F8GpBDCtoXjEcqDBFXNiCDaPNt1fekX2Po8uHWiEV
appTableGen = zb2rhjLU6zH29wMgUGmB4PMdVi84msArPtqjC1UwWVq61E5Ee
table =>
  max     = a => b => (if (gtn a b) a b)
  pos     = (get table "pos")
  mat     = (get table "mat")
  maxLens = (crossWith (txt => w => (max (len txt) w)) 0 mat)
  widths  = (map (mul 10) maxLens)
  heights = (map (x => 20) mat)
  (appTableGen {
    pos: (get table "pos")
    sizes: [widths heights]
    cell: i => j => (get (get mat (nts j)) (nts i))
  })

