{
  type: "app",
  name: "ethereum-account-manager",
  state: {
    accounts: []
  },
  render: env =>
    {
      type: "box"
      area: [0 0 (env "width") (env "height")]
      unselectable: 1
      children: [
        {
          type: "box"
          area: [20 20 200 40]
          children: [
            (con "Block Number: " (get (env "state") "blockNumber"))
          ]
        },
        {
          type: "box"
          area: [20 50 74 70]
          background: "rgb(200,200,200)"
          cursor: "pointer"
          onClick: ask => return =>
            (ask "eth" ["eth_blockNumber" ["latest"]] block =>
              (ask "setState" {blockNumber: block} then =>
                (return 0)))
          children: [
            "[update]"
          ]
        }
      ]
    }
}
